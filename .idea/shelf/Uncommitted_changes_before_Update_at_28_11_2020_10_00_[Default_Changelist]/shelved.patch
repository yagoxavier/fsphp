-        }
-
-        if ($returnCode == 127) {
-            throw new SystemRequirementsNotMetException('imagemagick is not installed');
-        }
-        if ($returnCode != 0) {
-            throw new SystemRequirementsNotMetException('The exec call failed');
-        }
