-
-        if ($ob_level != ob_get_level()) {
-            throw new FuncException('Output buffering level does not match when section was started.');
-        }
-
-        $contents = ob_get_clean();
-
-        $callback($contents);
-
-        $args->template()->with('buffer_stack', $buffer_stack);
-    };
