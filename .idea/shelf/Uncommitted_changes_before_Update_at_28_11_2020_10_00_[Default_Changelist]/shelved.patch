- */
-class Wpc extends AbstractConverter
-{
-    use CloudConverterTrait;
-    use CurlTrait;
-    use EncodingAutoTrait;
-
-    protected function getUnsupportedDefaultOptions()
-    {
-        return [];
-    }
-
-    public function supportsLossless()
